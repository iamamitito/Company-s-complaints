<tr id="company_<%=company.id%>">
        <td><%= company.name %></td>
        <td><%= link_to 'Detalle', company, class:"btn btn-dark" %></td>
        <td><%= link_to 'Editar', edit_company_path(company), data: {remote: true}, class:"btn btn-dark" %></td>
        <td><%= link_to 'Eliminar', company, method: :delete, data: { confirm: 'Are you sure?', remote: true }, class:"btn btn-dark" %></td>
</tr>